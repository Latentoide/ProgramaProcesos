<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.ies.lavereda.programaprocesos.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="apache" text="Apache" />
   <CheckBox fx:id="checkedA" mnemonicParsing="false" text="CheckBox" />
    <Label fx:id="idApache" text="" />
    <Button onAction="#onCreateNewProcessA" text="On!" />
    <Button onAction="#onStopA" text="Off" />
    <Button onAction="#onDestroyProcess" text="Destroy" />
    <Label fx:id="mariaDb" text="mariaDB" />
   <CheckBox fx:id="checkedM" mnemonicParsing="false" text="CheckBox" />
    <Label fx:id="idMaria" text="" />
    <Button onAction="#onCreateNewProcessM" text="On!" />
    <Button onAction="#onStopM" text="Off" />
    <Button onAction="#onDestroyProcess" text="Destroy" />
</VBox>
